{
  "name": "igniteui-sassdoc-theme",
  "description": "Default SassDoc theme used in Ignite UI for Angular.",
  "author": "Infragistics",
  "license": "MIT",
  "type": "module",
  "version": "0.1.0",
  "files": [
    "sassdoc/"
  ],
  "keywords": [
    "sassdoc",
    "theme",
    "styles api",
    "custom"
  ],
  "scripts": {
    "build": "vite build",
    "compile": "npm run build && sassdoc ./test/library -c ./test/sassdoc.config.json",
    "dev": "npm run compile && node ./scripts/copy-data.js && astro dev --host",
    "serve": "npm run compile && serve -p 8080 site"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "sassdoc": "^2.7.4",
    "serve": "^14.2.4",
    "vite": "^6.2.5",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-static-copy": "^2.3.0"
  },
  "dependencies": {
    "astro": "^5.6.0",
    "astro-breadcrumbs": "^3.3.1",
    "astro-expressive-code": "^0.40.2",
    "astro-pagefind": "^1.8.3",
    "fs-extra": "^11.3.0",
    "igniteui-theming": "^17.1.0",
    "sass-embedded": "^1.86.2",
    "typescript": "^5.8.2"
  }
}
